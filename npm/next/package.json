{
  "name": "@jansora/next",
  "version": "0.0.17",
  "description": "jansora next project management",
  "license": "MIT",
  "keywords": [
    "next",
    "jansora"
  ],
  "repository": "https://github.com/jansora/packages/npm/packages/next",
  "bugs": "https://github.com/jansora/packages/issues",
  "author": "Jansora <zhangyue1936@gmail.com> (https://www.jansora.com)",
  "files": [
    "package.json"
  ],
  "type": "module",
  "dependencies": {
    "@babel/runtime": "^7.18.6",
    "@jansora/bytemd": "0.0.11",
    "@jansora/monaco": "0.0.11",
    "@jansora/ui": "0.0.11",
    "ahooks": "^3.7.8",
    "autoprefixer": "10.4.14",
    "class-variance-authority": "^0.6.1",
    "clsx": "^1.2.1",
    "copy-to-clipboard": "^3.3.3",
    "dayjs": "^1.11.9",
    "lucide-react": "^0.257.0",
    "next": "14.0.4",
    "next-themes": "^0.2.1",
    "postcss": "8.4.24",
    "qs": "^6.11.2",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "sass": "^1.63.6",
    "set-cookie-parser": "^2.6.0",
    "sharp": "^0.32.6",
    "styled-components": "^5.2.1"
  },
  "scripts": {
    "bundle": "ncc build src/index.tsx --target web --filename codemirror && npm run bundle:min",
    "bundle:watch": "ncc watch src/index.tsx --target web --filename codemirror",
    "bundle:min": "ncc build src/index.tsx --target web --filename codemirror --minify",
    "watch": "tsbb watch src/*.tsx --use-babel",
    "build": "tsbb build src/*.tsx --use-babel",
    "css:build": "compile-less -d src -o esm && compile-less -d src -o cjs",
    "css:watch": "compile-less -d src -o esm --watch",
    "css:build:dist": "compile-less -d src --combine lib/dist.css --rm-global"
  },
  "xo": false,
  "publishConfig": {
    "access": "public"
  },
  "devDependencies": {
    "@types/react": "^18.2.46",
    "compile-less-cli": "^1.8.13"
  },
  "jest": {
    "coverageReporters": [
      "lcov",
      "json-summary"
    ]
  }
}
